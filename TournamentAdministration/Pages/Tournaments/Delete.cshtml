@page "{id:int}"
@model TournamentAdministration.Pages.Tournaments.DeleteModel

@{
    ViewData["Title"] = "Delete Tournament";
}

<h1>@ViewData["Title"]</h1>

<form method="post">
    <p>Are you sure you want to delete this tournament?</p>
    <button type="submit">Delete</button>
    <button type="submit" asp-page="./Index">Cancel</button>
</form>
